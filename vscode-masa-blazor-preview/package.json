{
  "name": "vscode-masa-blazor-preview",
  "displayName": "vscode-masa-blazor-preview",
  "description": "preivew masa blazor component",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:vscode-masa-blazor-preview.renderDocument"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vscode-masa-blazor-preview.renderDocument",
        "title": "Preview Razor File"
      }
    ],
    "menus": {
      "editor/title/run": [
        {
          "when": "resourceFilename =~ /.razor|.cshtml/",
          "command": "vscode-masa-blazor-preview.renderDocument",
          "group": "preview"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.0.1",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.74.0",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "@vscode/test-electron": "^2.2.2",
    "eslint": "^8.33.0",
    "glob": "^8.1.0",
    "mocha": "^10.1.0",
    "process": "^0.11.10",
    "tslib": "^2.5.0",
    "typescript": "^4.9.4",
    "util": "^0.12.5"
  },
  "dependencies": {
    "axios": "^1.3.0"
  }
}
